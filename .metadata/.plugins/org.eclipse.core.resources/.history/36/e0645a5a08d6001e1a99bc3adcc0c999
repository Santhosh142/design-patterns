package observerDesign;

public class NewsChannel implements Channel {
	
	String news;

	@Override
	public void update(Object o) {
		this.setNews((String) o);
		
	}

	private void setNews(String news) {
		this.news = news;
		
	}
	
	public String getNews() {
		return news + " from newsChannel";
	}

}
